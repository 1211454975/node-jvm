var util = require("util");

module.exports = {
    "print": function() {
        util.print.apply(null, arguments);
    },    
    "println": function() {
        util.print.apply(null, arguments);
        util.print("\n");
    },
    "format": function(fmt, args) {
        util.print(util.format.apply(null, [fmt].concat(args)));
    }
}